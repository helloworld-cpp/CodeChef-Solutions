#include <bits/stdc++.h>
using namespace std;

int main() {
	int t; cin>>t;
	while(t--){
	    
	    int a,b,i; cin>>a>>b;
	    if(a==b){return -1;}
	    else{
	    int c=abs(b-a);int count=0;
	            for(i=1;i<=sqrt(c);i++){
	                if(c%i==0){
	                    if(c/i==i){count++;}
	                    else{count =count +2;}
	                }
	                
	            }
	       cout<<count<<endl;
	    
	    }
	
	}
}

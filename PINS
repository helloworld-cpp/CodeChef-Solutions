#include<iostream>
#include<math.h>
#include <algorithm>

using namespace std;
int main(){
	int t,array[100000],i;
	cin>>t;
	    array[1]=1;
	    for(i=2;i<100000;i++){array[i]=0;}
	while(t--){
	    long int n;
	    cin>>n;
	    if (n==1){cout<<1<<" "<<1<<endl;}
	    else if((n%2)==0){cout<<1<<" ";
	        for(int i=1;i<=n/2+1;i++){
	            cout<<array[i];
	        }
	        cout<<endl;
	    }
	    else {cout<<1<<" ";
	        for(int i=1;i<=(n-1)/2+1;i++){
	            cout<<array[i];
	        }
	        cout<<endl;
	    }
	   
	}
		
	
}
# include<iostream>
#include <bits/stdc++.h>
#include<algorithm>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        long long int n,i,j;
        cin>>n;
        long long int a[n],res;
        set<long long int> s;
        for(i=0;i<n;i++){
            cin>>a[i];
            if(a[i]<=n){s.insert(a[i]);}
        }
        int size=s.size();
        res=n-size;
        
        
        
        cout<<res<<endl;
        
       
        
        
        
        
        
    }
}
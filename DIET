#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,k;
        cin>>n>>k;
        int a[n+1],i;
                    for(i=0;i<n;i++){
                        cin>>a[i];
                    }
                    a[n]=0;
                    int flag=1;
            for(i=0;i<n;i++){
                if(a[i]-k>=0){
                         a[i+1]=a[i]-k+a[i+1];
                }
                else {
                    cout<<"NO"<<" "<<i+1<<endl;
                    flag=0;
                    break;
                }
                
            }
            if (flag==1){
                cout<<"YES"<<endl;
            }
        
        
        
        
        
        
        
        
        
        
        
        
        
        
        
    }




	return 0;
}

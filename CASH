# include <bits/stdc++.h>
#define ll long long int
using namespace std;
int main(){
    int t; cin>>t;
    while(t--){
        ll n,k; cin>>n>>k;
        ll a[n],i,sum=0;
                for(i=0;i<n;i++){cin>>a[i];sum+=a[i];}
            sum=sum%k;
            cout<<sum<<endl;
    
      
      
      
    }
}
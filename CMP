#include<bits/stdc++.h>
using namespace std;

int main() {
	int a[3],b[3],ba=0,ca=0;
	cin>>a[0]>>a[1]>>a[2];
	cin>>b[0]>>b[1]>>b[2];
    for(int i=0;i<3;i++){
        if (a[i]>b[i]){ca++;}
        else if (a[i]<b[i]){ba++;}
        
        
    }
    cout<<ca<<" "<<ba<<endl;
	
	
	
	
	
}
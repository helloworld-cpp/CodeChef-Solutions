# include <bits/stdc++.h>
using namespace std;
int main(){
	int t;
	cin>>t;
	while(t--){
		long long int n,m,f=0,g=0;
		cin>>n;m=n;
			while(n>0){
				if(n==0){f++;n=n/10;}
				else break;
			}
			while(n>0){
				if(n%2==0){g++;n=n/2;}
				else break;
			}
			if(n==1){
				if (f>=g){cout<<"Yes"<<endl;}
				else {cout<<"No"<<endl;}
			}
			else{
				cout<<"No"<<endl;
			}

	}








}